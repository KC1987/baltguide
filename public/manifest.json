  "shortcuts": [
    {
      "name": "Search Attractions",
      "short_name": "Search",
      "description": "Search for attractions and places to visit",
      "url": "/map?search=true",
      "icons": [
        {
          "src": "/shortcut-search.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Offline Areas",
      "short_name": "Offline",
      "description": "View downloaded offline content",
      "url": "/offline",
      "icons": [
        {
          "src": "/shortcut-offline.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Favorites",
      "short_name": "Favorites",
      "description": "View your saved favorites",
      "url": "/favorites",
      "icons": [
        {
          "src": "/shortcut-favorites.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Near Me",
      "short_name": "Near Me",
      "description": "Find attractions near your location",
      "url": "/map?nearme=true",
      "icons": [
        {
          "src": "/shortcut-location.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  
  "categories": [
    "travel",
    "navigation",
    "tourism",
    "lifestyle",
    "utilities"
  ],
  
  "screenshots": [
    {
      "src": "/screenshot-mobile-1.png",
      "sizes": "540x720",
      "type": "image/png",
      "platform": "narrow",
      "label": "Mobile map interface with search and filters"
    },
    {
      "src": "/screenshot-mobile-2.png", 
      "sizes": "540x720",
      "type": "image/png",
      "platform": "narrow",
      "label": "Location details and offline capabilities"
    },
    {
      "src": "/screenshot-desktop-1.png",
      "sizes": "1280x800",
      "type": "image/png",
      "platform": "wide",
      "label": "Desktop view with advanced filters"
    }
  ],
  
  "related_applications": [],
  "prefer_related_applications": false,
  
  "protocol_handlers": [
    {
      "protocol": "web+baltguide",
      "url": "/?handler=%s"
    }
  ],
  
  "edge_side_panel": {
    "preferred_width": 400
  },
  
  "file_handlers": [
    {
      "action": "/handle-file",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"]
      }
    }
  ],
  
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "images",
          "accept": ["image/*"]
        }
      ]
    }
  },
  
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  
  "handle_links": "preferred",
  
  "display_override": [
    "window-controls-overlay",
    "minimal-ui",
    "standalone",
    "browser"
  ],
  
  "id": "baltguide-travel-app",
  
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7"
}